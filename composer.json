            "name": "Amsgames, LLC",
            "email": "support@amsgames.com"
        },
        {
            "name": "Alejandro Mostajo",
            "email": "amostajo@gmail.com"
        }
        ,
        {
            "name": "Mostafa Elbakly",
            "email": "elbakly@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/console": "*",
        "illuminate/support": "*"
    },
    "autoload": {
        "classmap": [
            "src/Commands"
        ],
        "psr-4": {
            "Amsgames\\LaravelShop\\": "src"
        }
    },
    "minimum-stability": "dev"
}
